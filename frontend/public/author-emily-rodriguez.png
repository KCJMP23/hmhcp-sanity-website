<svg width="400" height="400" viewBox="0 0 400 400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <radialGradient id="bgGradient" cx="50%" cy="30%">
      <stop offset="0%" style="stop-color:#f0f9ff"/>
      <stop offset="100%" style="stop-color:#dbeafe"/>
    </radialGradient>
    <linearGradient id="avatarGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#3b82f6"/>
      <stop offset="100%" style="stop-color:#1e40af"/>
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="400" height="400" fill="url(#bgGradient)"/>
  
  <!-- Professional avatar circle -->
  <circle cx="200" cy="160" r="80" fill="url(#avatarGradient)" opacity="0.1"/>
  <circle cx="200" cy="160" r="75" fill="#f8fafc" stroke="#3b82f6" stroke-width="3"/>
  
  <!-- Professional silhouette -->
  <path d="M160 140 Q160 120 180 120 Q200 115 220 120 Q240 120 240 140 Q240 160 220 170 Q200 175 180 170 Q160 160 160 140 Z" fill="#3b82f6" opacity="0.8"/>
  <rect x="175" y="170" width="50" height="40" rx="5" fill="#3b82f6" opacity="0.8"/>
  
  <!-- Medical cross symbol -->
  <g transform="translate(320, 80)">
    <circle r="25" fill="#10b981" opacity="0.9"/>
    <rect x="-12" y="-3" width="24" height="6" fill="white"/>
    <rect x="-3" y="-12" width="6" height="24" fill="white"/>
  </g>
  
  <!-- Stethoscope icon -->
  <g transform="translate(80, 80)" fill="#6b7280" opacity="0.6">
    <circle cx="0" cy="0" r="8" fill="none" stroke="#6b7280" stroke-width="2"/>
    <path d="M 0 8 Q 15 20 25 35" stroke="#6b7280" stroke-width="3" fill="none"/>
    <circle cx="25" cy="35" r="5"/>
  </g>
  
  <!-- Professional name plate -->
  <rect x="50" y="280" width="300" height="80" rx="10" fill="white" opacity="0.95" stroke="#e5e7eb"/>
  <text x="200" y="310" text-anchor="middle" font-family="Arial, sans-serif" font-size="20" font-weight="bold" fill="#1f2937">Dr. Emily Rodriguez</text>
  <text x="200" y="335" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" fill="#6b7280">Chief Medical Officer</text>
  <text x="200" y="350" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#9ca3af">Healthcare Innovation Expert</text>
</svg>