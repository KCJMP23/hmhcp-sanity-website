  </defs>
  
  <!-- Background -->
  <rect width="400" height="400" fill="url(#bgGradient2)"/>
  
  <!-- Professional avatar circle -->
  <circle cx="200" cy="160" r="80" fill="url(#avatarGradient2)" opacity="0.1"/>
  <circle cx="200" cy="160" r="75" fill="#f8fafc" stroke="#10b981" stroke-width="3"/>
  
  <!-- Professional silhouette -->
  <path d="M160 140 Q160 120 180 120 Q200 115 220 120 Q240 120 240 140 Q240 160 220 170 Q200 175 180 170 Q160 160 160 140 Z" fill="#10b981" opacity="0.8"/>
  <rect x="175" y="170" width="50" height="40" rx="5" fill="#10b981" opacity="0.8"/>
  
  <!-- Medical chart symbol -->
  <g transform="translate(320, 80)">
    <rect x="-15" y="-20" width="30" height="40" rx="3" fill="#3b82f6" opacity="0.9"/>
    <rect x="-10" y="-15" width="20" height="3" fill="white"/>
    <rect x="-10" y="-8" width="15" height="2" fill="white"/>
    <rect x="-10" y="-3" width="18" height="2" fill="white"/>
    <rect x="-10" y="2" width="12" height="2" fill="white"/>
  </g>
  
  <!-- Analytics/graph icon -->
  <g transform="translate(80, 80)" fill="#6b7280" opacity="0.6">
    <rect x="-10" y="10" width="4" height="15" fill="#10b981"/>
    <rect x="-4" y="5" width="4" height="20" fill="#3b82f6"/>
    <rect x="2" y="8" width="4" height="17" fill="#8b5cf6"/>
    <rect x="8" y="0" width="4" height="25" fill="#f59e0b"/>
