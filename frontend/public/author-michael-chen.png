  <!-- Professional name plate -->
  <rect x="50" y="280" width="300" height="80" rx="10" fill="white" opacity="0.95" stroke="#e5e7eb"/>
  <text x="200" y="310" text-anchor="middle" font-family="Arial, sans-serif" font-size="20" font-weight="bold" fill="#1f2937">Dr. Michael Chen</text>
  <text x="200" y="335" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" fill="#6b7280">Data Science Director</text>
  <text x="200" y="350" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#9ca3af">Healthcare Analytics Expert</text>
