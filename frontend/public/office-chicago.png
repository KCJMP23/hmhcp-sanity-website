<svg width="600" height="400" viewBox="0 0 600 400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="skyGradientChi" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#fef3c7"/>
      <stop offset="100%" style="stop-color:#fed7aa"/>
    </linearGradient>
    <linearGradient id="buildingGradientChi" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#fffbeb"/>
      <stop offset="100%" style="stop-color:#fde68a"/>
    </linearGradient>
  </defs>
  
  <!-- Sky background -->
  <rect width="600" height="270" fill="url(#skyGradientChi)"/>
  
  <!-- Lake/water -->
  <rect y="270" width="600" height="130" fill="#3b82f6" opacity="0.3"/>
  
  <!-- Main building - modern style -->
  <rect x="180" y="70" width="240" height="200" fill="url(#buildingGradientChi)" stroke="#d97706" stroke-width="2"/>
  
  <!-- Glass facade effect -->
  <rect x="190" y="80" width="220" height="180" fill="#3b82f6" opacity="0.1"/>
  
  <!-- Modern window pattern -->
  <g fill="#1e40af" opacity="0.7">
    <!-- Large windows -->
    <rect x="200" y="90" width="40" height="60" rx="3"/>
    <rect x="250" y="90" width="40" height="60" rx="3"/>
    <rect x="300" y="90" width="40" height="60" rx="3"/>
    <rect x="350" y="90" width="40" height="60" rx="3"/>
    
    <rect x="200" y="160" width="40" height="60" rx="3"/>
    <rect x="250" y="160" width="40" height="60" rx="3"/>
    <rect x="300" y="160" width="40" height="60" rx="3"/>
    <rect x="350" y="160" width="40" height="60" rx="3"/>
    
    <rect x="200" y="230" width="40" height="30" rx="3"/>
    <rect x="250" y="230" width="40" height="30" rx="3"/>
    <rect x="300" y="230" width="40" height="30" rx="3"/>
    <rect x="350" y="230" width="40" height="30" rx="3"/>
  </g>
  
  <!-- Modern entrance -->
  <rect x="275" y="200" width="50" height="70" fill="#374151"/>
  <rect x="285" y="210" width="30" height="60" fill="#60a5fa" opacity="0.4"/>
  
  <!-- Side structures -->
  <rect x="70" y="150" width="90" height="120" fill="#f3f4f6" stroke="#d97706"/>
  <rect x="440" y="140" width="110" height="130" fill="#f3f4f6" stroke="#d97706"/>
  
  <!-- Chicago architectural accent -->
  <rect x="175" y="60" width="250" height="20" fill="#d97706"/>
  <rect x="170" y="55" width="260" height="10" fill="#92400e"/>
  
  <!-- Healthcare symbol -->
  <g transform="translate(300, 40)">
    <circle r="18" fill="#059669"/>
    <rect x="-10" y="-2" width="20" height="4" fill="white"/>
    <rect x="-2" y="-10" width="4" height="20" fill="white"/>
  </g>
  
  <!-- Reflection in water -->
  <g opacity="0.3">
    <rect x="180" y="290" width="240" height="80" fill="url(#buildingGradientChi)"/>
  </g>
  
  <!-- Waterfront elements -->
  <ellipse cx="100" cy="350" rx="30" ry="15" fill="#1e40af" opacity="0.5"/>
  <ellipse cx="500" cy="360" rx="25" ry="12" fill="#1e40af" opacity="0.5"/>
  
  <!-- Sign -->
  <rect x="150" y="340" width="300" height="40" rx="5" fill="white" stroke="#d97706" stroke-width="2"/>
  <text x="300" y="360" text-anchor="middle" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#1f2937">HM Healthcare Partners Chicago</text>
  <text x="300" y="375" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#6b7280">Lakefront Medical Center</text>
</svg>