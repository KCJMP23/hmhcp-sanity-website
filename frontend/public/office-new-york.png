  <!-- Sign -->
  <rect x="180" y="350" width="240" height="35" rx="5" fill="white" stroke="#3b82f6" stroke-width="2"/>
  <text x="300" y="368" text-anchor="middle" font-family="Arial, sans-serif" font-size="13" font-weight="bold" fill="#1f2937">HM Healthcare Partners NYC</text>
  <text x="300" y="380" text-anchor="middle" font-family="Arial, sans-serif" font-size="9" fill="#6b7280">Manhattan Medical Tower</text>
