<svg width="600" height="400" viewBox="0 0 600 400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="precognitiveBg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f0f4ff;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e0e7ff;stop-opacity:1" />
    </linearGradient>
    <pattern id="brainPattern" x="0" y="0" width="50" height="50" patternUnits="userSpaceOnUse">
      <circle cx="25" cy="25" r="1" fill="rgba(59, 130, 246, 0.1)"/>
      <path d="M12 25 Q25 12 38 25 Q25 38 12 25" stroke="rgba(59, 130, 246, 0.1)" stroke-width="0.5" fill="none"/>
    </pattern>
  </defs>
  
  <!-- Background -->
  <rect width="600" height="400" fill="url(#precognitiveBg)"/>
  <rect width="600" height="400" fill="url(#brainPattern)" opacity="0.3"/>
  
  <!-- Dashboard Frame -->
  <rect x="50" y="60" width="500" height="280" rx="12" fill="white" stroke="#c7d2fe" stroke-width="2"/>
  
  <!-- Header -->
  <rect x="50" y="60" width="500" height="45" rx="12" fill="#4338ca"/>
  <text x="70" y="85" font-family="system-ui, sans-serif" font-size="18" font-weight="600" fill="white">Precognitive Health Dashboard</text>
  
  <!-- AI Brain Icon -->
  <g transform="translate(480, 70)">
    <circle cx="15" cy="15" r="12" fill="rgba(255,255,255,0.2)"/>
    <path d="M8 15 Q15 8 22 15 Q15 22 8 15" stroke="white" stroke-width="1.5" fill="none"/>
    <circle cx="15" cy="15" r="2" fill="white"/>
  </g>
  
  <!-- Main Content Area -->
  <g transform="translate(70, 125)">
    <!-- Predictive Analytics Cards -->
    <rect x="0" y="0" width="140" height="80" rx="8" fill="#e0e7ff"/>
    <text x="10" y="20" font-family="system-ui, sans-serif" font-size="12" font-weight="600" fill="#4338ca">Risk Prediction</text>
    <text x="10" y="45" font-family="system-ui, sans-serif" font-size="24" font-weight="700" fill="#1e40af">87.3%</text>
    <text x="10" y="65" font-family="system-ui, sans-serif" font-size="10" fill="#6366f1">Accuracy Rate</text>
    
    <rect x="160" y="0" width="140" height="80" rx="8" fill="#dbeafe"/>
    <text x="170" y="20" font-family="system-ui, sans-serif" font-size="12" font-weight="600" fill="#1d4ed8">Early Detection</text>
    <text x="170" y="45" font-family="system-ui, sans-serif" font-size="24" font-weight="700" fill="#2563eb">342</text>
    <text x="170" y="65" font-family="system-ui, sans-serif" font-size="10" fill="#3b82f6">Cases Identified</text>
    
    <rect x="320" y="0" width="140" height="80" rx="8" fill="#fef3c7"/>
    <text x="330" y="20" font-family="system-ui, sans-serif" font-size="12" font-weight="600" fill="#d97706">Prevention Score</text>
    <text x="330" y="45" font-family="system-ui, sans-serif" font-size="24" font-weight="700" fill="#f59e0b">94.2%</text>
    <text x="330" y="65" font-family="system-ui, sans-serif" font-size="10" fill="#fbbf24">Effectiveness</text>
    
    <!-- Neural Network Visualization -->
    <g transform="translate(0, 100)">
      <rect x="0" y="0" width="460" height="100" rx="8" fill="white" stroke="#e5e7eb"/>
      <text x="10" y="20" font-family="system-ui, sans-serif" font-size="14" font-weight="600" fill="#374151">Neural Network Activity</text>
      
      <!-- Network nodes -->
      <g opacity="0.8">
        <!-- Input layer -->
        <circle cx="50" cy="40" r="4" fill="#4338ca"/>
        <circle cx="50" cy="60" r="4" fill="#4338ca"/>
        <circle cx="50" cy="80" r="4" fill="#4338ca"/>
        
        <!-- Hidden layer -->
        <circle cx="150" cy="35" r="4" fill="#6366f1"/>
        <circle cx="150" cy="50" r="4" fill="#6366f1"/>
        <circle cx="150" cy="65" r="4" fill="#6366f1"/>
        <circle cx="150" cy="80" r="4" fill="#6366f1"/>
        
        <!-- Output layer -->
        <circle cx="250" cy="50" r="4" fill="#8b5cf6"/>
        <circle cx="250" cy="70" r="4" fill="#8b5cf6"/>
        
        <!-- Connections -->
        <line x1="54" y1="40" x2="146" y2="35" stroke="#4338ca" stroke-width="1" opacity="0.4"/>
        <line x1="54" y1="60" x2="146" y2="50" stroke="#4338ca" stroke-width="1" opacity="0.4"/>
        <line x1="54" y1="80" x2="146" y2="65" stroke="#4338ca" stroke-width="1" opacity="0.4"/>
        <line x1="154" y1="50" x2="246" y2="50" stroke="#6366f1" stroke-width="1" opacity="0.4"/>
        <line x1="154" y1="65" x2="246" y2="70" stroke="#6366f1" stroke-width="1" opacity="0.4"/>
      </g>
      
      <!-- Data flow animation -->
      <g opacity="0.6">
        <circle cx="300" cy="60" r="2" fill="#10b981">
          <animate attributeName="cx" values="300;350;400;450" dur="2s" repeatCount="indefinite"/>
          <animate attributeName="opacity" values="0.6;1;0.6;0" dur="2s" repeatCount="indefinite"/>
        </circle>
      </g>
    </g>
  </g>
  
  <!-- Title -->
  <text x="300" y="40" text-anchor="middle" font-family="system-ui, sans-serif" font-size="20" font-weight="600" fill="#4338ca">AI-Powered Health Prediction</text>
</svg>