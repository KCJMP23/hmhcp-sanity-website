<svg width="600" height="400" viewBox="0 0 600 400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="dashboardBg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f8fafc;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e2e8f0;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="600" height="400" fill="url(#dashboardBg)"/>
  
  <!-- Dashboard Frame -->
  <rect x="50" y="50" width="500" height="300" rx="10" fill="white" stroke="#e2e8f0" stroke-width="2"/>
  
  <!-- Header Bar -->
  <rect x="50" y="50" width="500" height="40" rx="10" fill="#1e293b"/>
  <text x="70" y="75" font-family="system-ui, sans-serif" font-size="16" font-weight="600" fill="white">Intellic Platform</text>
  
  <!-- Navigation dots -->
  <circle cx="480" cy="70" r="4" fill="#ef4444"/>
  <circle cx="500" cy="70" r="4" fill="#f59e0b"/>
  <circle cx="520" cy="70" r="4" fill="#10b981"/>
  
  <!-- Dashboard Content -->
  <g transform="translate(70, 110)">
    <!-- Metrics Cards -->
    <rect x="0" y="0" width="100" height="60" rx="5" fill="#3b82f6" opacity="0.1"/>
    <text x="10" y="20" font-family="system-ui, sans-serif" font-size="12" font-weight="600" fill="#1e293b">Active Patients</text>
    <text x="10" y="45" font-family="system-ui, sans-serif" font-size="20" font-weight="700" fill="#3b82f6">1,247</text>
    
    <rect x="120" y="0" width="100" height="60" rx="5" fill="#10b981" opacity="0.1"/>
    <text x="130" y="20" font-family="system-ui, sans-serif" font-size="12" font-weight="600" fill="#1e293b">Outcomes</text>
    <text x="130" y="45" font-family="system-ui, sans-serif" font-size="20" font-weight="700" fill="#10b981">94.2%</text>
    
    <rect x="240" y="0" width="100" height="60" rx="5" fill="#f59e0b" opacity="0.1"/>
    <text x="250" y="20" font-family="system-ui, sans-serif" font-size="12" font-weight="600" fill="#1e293b">Alerts</text>
    <text x="250" y="45" font-family="system-ui, sans-serif" font-size="20" font-weight="700" fill="#f59e0b">23</text>
    
    <!-- Chart Area -->
    <rect x="0" y="80" width="460" height="140" rx="5" fill="white" stroke="#e5e7eb"/>
    
    <!-- Chart Content -->
    <g transform="translate(20, 100)">
      <!-- Y-axis -->
      <line x1="0" y1="0" x2="0" y2="100" stroke="#d1d5db" stroke-width="1"/>
      <!-- X-axis -->
      <line x1="0" y1="100" x2="420" y2="100" stroke="#d1d5db" stroke-width="1"/>
      
      <!-- Data bars -->
      <rect x="20" y="70" width="30" height="30" fill="#3b82f6" opacity="0.8"/>
      <rect x="70" y="50" width="30" height="50" fill="#3b82f6" opacity="0.8"/>
      <rect x="120" y="60" width="30" height="40" fill="#3b82f6" opacity="0.8"/>
      <rect x="170" y="40" width="30" height="60" fill="#3b82f6" opacity="0.8"/>
      <rect x="220" y="65" width="30" height="35" fill="#3b82f6" opacity="0.8"/>
      <rect x="270" y="45" width="30" height="55" fill="#3b82f6" opacity="0.8"/>
      <rect x="320" y="55" width="30" height="45" fill="#3b82f6" opacity="0.8"/>
      <rect x="370" y="35" width="30" height="65" fill="#3b82f6" opacity="0.8"/>
    </g>
  </g>
  
  <!-- AI Indicators -->
  <g opacity="0.6">
    <circle cx="450" cy="150" r="8" fill="#8b5cf6"/>
    <text x="450" y="155" text-anchor="middle" font-family="system-ui, sans-serif" font-size="8" fill="white" font-weight="600">AI</text>
  </g>
</svg>